# OpenVPN Server build v
# Server Location: TH, Bangkok
# Server ISP: SheepLink Co.,Ltd.
#
# Convert your IP address into hostname (class A record) combined with Twitter's URL to make this config work
# example: api.twitter.com.mydns.domain.com

client
dev tun
persist-tun
proto tcp
remote oppo 1 udp
remote 103.2.115.71 110
http-proxy 103.2.115.71 8080
persist-remote-ip
resolv-retry infinite
connect-retry 0 1
remote-cert-tls server
nobind
reneg-sec 0
keysize 0
rcvbuf 0
sndbuf 0
verb 2
comp-lzo
auth none
auth-nocache
cipher none
setenv CLIENT_CERT 0
http-proxy-option VERSION 1.1
http-proxy-option AGENT Chrome/80.0.3987.87
http-proxy-option CUSTOM-HEADER CONNECT HTTP/1.1
http-proxy-option CUSTOM-HEADER Host "www.opensignal.com/ HTTP/1.1\r\n\r\n"
http-proxy-option CUSTOM-HEADER X-Online-Host "www.opensignal.com/ HTTP/1.1\r\n\r\n"
http-proxy-option CUSTOM-HEADER X-Forward-Host "www.opensignal.com/ HTTP/1.1\r\n\r\n"
http-proxy-option CUSTOM-HEADER Connection Keep-Alive
http-proxy-option CUSTOM-HEADER Upgrade-Insecure-Requests: 2
dhcp-option "subdomain":"" www.opendns.com"
dhcp-option "subdomain":"" www.opensignal.com"
dhcp-option "subdomain":"" app.dtac.co.th"
route 141.98.16.154 255.255.255.255 net_gateway
dhcp-option DNS "8.8.8.8"
dhcp-option DNS "8.8.4.4"
dhcp-option DNS "1.1.1.1"
dhcp-option DNS "1.0.0.1"
dhcp-option DNS "2001:4860:4860::8888"
